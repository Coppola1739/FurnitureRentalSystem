<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="700.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.westga.cs3230.furniturerentalsystem.controller.MemberController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="657.0" prefWidth="400.0" VBox.vgrow="ALWAYS">
         <children>
            <ListView fx:id="memberListView" layoutX="230.0" layoutY="57.0" prefHeight="418.0" prefWidth="256.0" />
            <TextField fx:id="memberSearchTextField" layoutX="19.0" layoutY="89.0" />
            <Label fx:id="memberUserNameLabel" layoutX="7.0" layoutY="6.0" prefHeight="17.0" prefWidth="184.0" />
            <Label fx:id="MembersListLabel" layoutX="230.0" layoutY="36.0" prefHeight="17.0" prefWidth="191.0" text="Members:" />
            <Button fx:id="registerMemberButton" layoutX="366.0" layoutY="475.0" mnemonicParsing="false" onAction="#registerMember" text="Register Member" />
            <Button fx:id="editMemberButton" layoutX="250.0" layoutY="475.0" mnemonicParsing="false" onAction="#editMember" prefHeight="26.0" prefWidth="111.0" text="Edit Member" />
            <ComboBox fx:id="searchFilterComboBox" layoutX="19.0" layoutY="57.0" prefHeight="26.0" prefWidth="160.0" />
            <Button fx:id="SearchButton" layoutX="43.0" layoutY="116.0" mnemonicParsing="false" onAction="#searchForMembers" text="Search" />
            <Button fx:id="logOutButton" layoutX="439.0" layoutY="2.0" mnemonicParsing="false" onAction="#logOut" prefHeight="26.0" prefWidth="61.0" text="Log Out" />
            <Button fx:id="ClearSearchButton" layoutX="99.0" layoutY="116.0" mnemonicParsing="false" onAction="#clearMemberSearch" text="Clear" />
            <Label fx:id="SearchFilterLabel" layoutX="19.0" layoutY="36.0" prefHeight="17.0" prefWidth="160.0" text="Search Filter:" />
            <Button fx:id="homeButton" layoutX="388.0" layoutY="2.0" mnemonicParsing="false" onAction="#navigateToHomePage" prefHeight="26.0" prefWidth="51.0" text="Home" />
         </children>
      </AnchorPane>
   </children>
</VBox>
